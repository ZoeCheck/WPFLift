<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:WPFLift" x:Class="WPFLift.MainWindow"
        Title="ElevatorApp" Height="694" Width="488"
        WindowStartupLocation="CenterScreen"
        Icon="/WPFLift;component/Images/933.ico"
        Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
    	<Grid.RowDefinitions>
    		<RowDefinition Height="0.498*"/>
    		<RowDefinition Height="0.074*"/>
    		<RowDefinition Height="0.149*"/>
    		<RowDefinition Height="0.278*"/>
    		<RowDefinition Height="25"/>
    	</Grid.RowDefinitions>
    	<StackPanel HorizontalAlignment="Left" Width="184" Margin="0" Grid.RowSpan="4">
    		<local:UCFloor Margin="0"/>
    		<local:UCFloor Margin="0"/>
    		<local:UCFloor Margin="0"/>
    		<local:UCFloor Margin="0"/>
    	</StackPanel>
    	<Grid x:Name="GridBottomInfo" Margin="0" Grid.Row="4" Background="Gainsboro">
    		<StackPanel HorizontalAlignment="Right" Margin="0,0,2,0" Orientation="Horizontal" VerticalAlignment="Center">
    			<TextBlock TextWrapping="Wrap" Text="版本" VerticalAlignment="Center" Foreground="#FF07642A"/>
    			<TextBlock x:Name="tblVersion" Margin="5,0,0,0" TextWrapping="Wrap" Text="1.0.5100.10000" VerticalAlignment="Center" Foreground="#FF07642A"/>
    		</StackPanel>
    	</Grid>
        
    </Grid>
</Window>
